{
    "manifest_version": 3,
    "name": "Shopee Seller Auto",
    "version": "1.0",
    "description": "Automate duplicated actions in Shopee Seller",
    "permissions": ["scripting", "activeTab"],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Shopee Auto"
    }
}